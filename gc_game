DROP TABLE IF EXISTS `gc_game`;

CREATE TABLE `gc_game`(
    `gameID` nt NOT NULL AUTO_INCREMENT,
    `gameTitle` varchar(255) NOT NULL,
    `ATH` float NOT NULL,        /*ALL TIME HIGH*/
    `ATL` float NOT NULL,        /*ALL TIME LOW*/
    `monthHigh` float NOT NULL,
    `monthLow`  float NOT NULL,
    `currentPrice` float NOT NULL,
    `priceflux` float NOT NULL,  /*PERCENTAGE CHANGE IN PRICE OVER THE MONTH*/
    PRIMARY KEY(`gameID`)
)
